			<div class="profile-right-column">
				<div class="friend-request-container">
					<%= render partial: "friend_requests/request" %>
				</div>

			</div> <!-- end profile-right-column --> 

		</div> <!-- end inner-profile-body --> 
	</div> <!-- end profile-body --> 

