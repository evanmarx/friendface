<%= form_tag(session_path(@user), class: 'muted form-inline pull-right') do %>
	<%= text_field_tag :email, nil, :autofocus => true, class:"input-small", placeholder:"Name" %>
	<%= password_field_tag :password, nil, class:"input-small", placeholder:"Password" %>
	<%= submit_tag "Log In", class: "btn", id: "nbl"%>
<% end %>


